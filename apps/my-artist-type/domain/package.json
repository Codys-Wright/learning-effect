{
  "name": "@my-artist-type/domain",
  "version": "0.0.0",
  "type": "module",
  "license": "MIT",
  "description": "The domain template",
  "repository": {
    "type": "git",
    "url": "https://github.com/lucas-barake/rpc-tutorial.git",
    "directory": "apps/my-artist-type/domain"
  },
  "exports": {
    ".": {
      "default": "./src/index.ts",
      "production": "./build/esm/index.js"
    },
    "./package.json": "./package.json",
    "./*": {
      "default": "./src/*.ts",
      "production": "./build/esm/*.js"
    }
  },
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
    "build-annotate": "babel build/esm --plugins annotate-pure-calls --out-dir build/esm --source-maps",
    "check": "tsc -b tsconfig.json",
    "test": "vitest --run",
    "coverage": "vitest --coverage"
  },
  "dependencies": {
    "@effect/platform": "*",
    "@features/quiz": "workspace:^",
    "@effect/rpc": "*",
    "@faker-js/faker": "^10.0.0",
    "effect": "*"
  },
  "effect": {
    "generateExports": {
      "include": [
        "**/*.ts"
      ]
    },
    "generateIndex": {
      "include": [
        "**/*.ts"
      ]
    }
  },
  "devDependencies": {
    "@effect/build-utils": "^0.8.3"
  }
}
